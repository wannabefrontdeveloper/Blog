{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended","prettier"],
    "globals": {
        "Atomics": "readonly",
        "ShareArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module"
    },
    
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off"
    }
}
